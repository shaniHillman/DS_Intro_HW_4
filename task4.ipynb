    "        else: #status_code=200\n",
    "            try:\n",
    "                response_data = response.json()\n",
    "                #print(response_data)\n",
    "                return(response_data['results'][0]['geometry']['location'])\n",
    "            except:\n",
    "                print(\"Response not in valid JSON format\")\n",
